{
  "name": "@0xsequence/config",
  "version": "0.36.13",
  "description": "config sub-package for Sequence",
  "repository": "https://github.com/0xsequence/sequence.js/tree/master/packages/config",
  "source": "src/index.ts",
  "main": "dist/0xsequence-config.cjs.js",
  "module": "dist/0xsequence-config.esm.js",
  "author": "Horizon Blockchain Games",
  "license": "Apache-2.0",
  "scripts": {
    "test": "pnpm test:concurrently 'pnpm test:run'",
    "test:run": "pnpm test:file tests/**/*.spec.ts",
    "test:file": "TS_NODE_PROJECT=../../tsconfig.test.json mocha -r ts-node/register --timeout 30000",
    "test:concurrently": "concurrently -k --success first 'pnpm start:hardhat_b > /dev/null' 'pnpm start:hardhat_a > /dev/null' ",
    "start:hardhat_a": "hardhat node --port 7547",
    "start:hardhat_b": "hardhat node --port 7548",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@0xsequence/abi": "^0.36.13",
    "@0xsequence/multicall": "^0.36.13",
    "@0xsequence/network": "^0.36.13",
    "@0xsequence/utils": "^0.36.13",
    "ethers": "^5.5.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@0xsequence/wallet-contracts": "1.9.6",
    "@ethersproject/providers": "^5.5.0"
  },
  "files": [
    "src",
    "dist"
  ]
}
